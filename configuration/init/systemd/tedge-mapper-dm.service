[Unit]
Description=tedge-mapper-dm converts Thin Edge JSON measurements to Cumulocity JSON format.
After=syslog.target network.target mosquitto.service

[Service]
User=tedge-mapper
ExecStart=/usr/bin/tedge_mapper dm
Restart=on-failure
RestartPreventExitStatus=255

[Install]
WantedBy=multi-user.target
